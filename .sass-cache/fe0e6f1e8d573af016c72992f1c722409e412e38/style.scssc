	/* body */
	body {
		font-family: 'Droid Sans', Arial, Helvetica, sans-serif;
		font-size:12px;
		line-height:20px; 
		position:relative;
		background:#E5E5E5;
		color:#666;
		padding:0 0 0 1px;
	}

	/* Layout general*/
#container
{	width:100%;
	max-width:1990px;
	margin:0 auto 0 auto; 
	position:relative;
	display: block;
	padding:30px 0px	0px	0px;
	border:0 !important; 
}
	
	/* Layout general*/
	#wrapper{
		width:960px;
		margin:0 auto 0 auto; 
		position:relative;
		display: block;
		padding:0;
		border:0 !important;
		z-index:4;
	}
	
	/* background border */
	.background_pic_border{
		max-width:1990px;
		position:absolute;
		top:0;
		width:100%;
		text-align:center;
		z-index:1;
		overflow:hidden;
	}
	
	/* background image holder */
	.background_pic_holder{
		margin:0 auto 0 auto;
		max-width:1990px;	
		min-width:980px;
		width:100%;
		overflow:hidden;
		max-height:600px;
		text-align:center;
		z-index:2;
	} 

	/* background image magin */
	.background_pic_holder img{ 
		margin:0 auto;
	}  

	/* background curv image */	
	.back-curv{
		background:url(../images/back-curv.png) center top no-repeat;	
		height:452px;
		width:100%;
		position:relative;
		z-index:99;
		top:-280px;
		min-width:980px;
		z-index:3;
	}

	/* background line image */	
	.back-line{
		background:url(../images/back-line.png) center top repeat-x;	
		height:160px;
		width:100%;
		position:relative;
		z-index:99;
